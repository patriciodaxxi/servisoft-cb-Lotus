  object qPedidos: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT Dbcliente.Nome, Dbproduto.Referencia, SUM( Dbpedidoitem.Q' +
        'tdPares ) QtdPares'
      'FROM "dbPedido.DB" Dbpedido'
      '   INNER JOIN "dbCliente.DB" Dbcliente'
      '   ON  (Dbpedido.CodCliente = Dbcliente.Codigo)  '
      '   INNER JOIN "dbPedidoItem.DB" Dbpedidoitem'
      '   ON  (Dbpedido.Pedido = Dbpedidoitem.Pedido)  '
      '   INNER JOIN "dbProduto.DB" Dbproduto'
      '   ON  (Dbpedidoitem.CodProduto = Dbproduto.Codigo)  '
      'WHERE   (Dbpedido.Cancelado <> TRUE)  '
      '   AND  (Dbpedidoitem.Cancelado <> TRUE)  '
      '   AND  Dbpedido.DtEmissao BETWEEN :Data1 AND :Data2'
      'GROUP BY Dbcliente.Nome, Dbproduto.Referencia'
      'ORDER BY Dbcliente.Nome, Dbproduto.Referencia')
    Left = 208
    Top = 8
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'Data1'
        ParamType = ptUnknown
        Value = 0d
      end
      item
        DataType = ftDateTime
        Name = 'Data2'
        ParamType = ptUnknown
        Value = 0d
      end>
    object qPedidosNome: TStringField
      FieldName = 'Nome'
      Size = 40
    end
    object qPedidosReferencia: TStringField
      FieldName = 'Referencia'
      Size = 8
    end
    object qPedidosQtdPares: TFloatField
