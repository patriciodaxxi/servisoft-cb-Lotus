  object qInterno: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT Dblotemi.Lote, Dbpedidomaterial.CodMaterial, Dbpedidomate' +
        'rial.CodCor, Dbpedidomaterial.ConsumoPr, Dbpedidoitem.QtdPares, ' +
        'Dbmaterial.Unidade'
      'FROM "dbLoteMI.DB" Dblotemi'
      '   INNER JOIN "dbLotePedidoMI.DB" Dblotepedidomi'
      '   ON  (Dblotemi.Lote = Dblotepedidomi.Lote)  '
      '   INNER JOIN "dbPedido.DB" Dbpedido'
      '   ON  (Dblotepedidomi.Pedido = Dbpedido.Pedido)  '
      '   INNER JOIN "dbPedidoItem.DB" Dbpedidoitem'
      '   ON  (Dbpedido.Pedido = Dbpedidoitem.Pedido)  '
      '   INNER JOIN "DBPEDIDOMATERIAL.DB" Dbpedidomaterial'
      '   ON  (Dbpedidoitem.Pedido = Dbpedidomaterial.Pedido)  '
      '   AND  (Dbpedidoitem.Item = Dbpedidomaterial.ItemPed)  '
      '   INNER JOIN "dbMaterial.DB" Dbmaterial'
      '   ON  (Dbpedidomaterial.CodMaterial = Dbmaterial.Codigo)  ')
    Left = 40
    Top = 448
  end
