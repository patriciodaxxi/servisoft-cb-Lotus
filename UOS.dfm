  object qExportacao: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT Dblote.Lote, Dblote.QtdPares, Dbpedidomaterial.CodMateria' +
        'l, Dbpedidomaterial.CodCor, Dbpedidomaterial.ConsumoPr, Dbmateri' +
        'al.Unidade, dbPedidoItem.Pedido, dbPedidoItem.Item, dbGrade.Larg' +
        'ura, dbPedidoItem.CodGrade'
      'FROM "dbLote.DB" Dblote'
      '   INNER JOIN "dbPedidoItem.DB" Dbpedidoitem'
      '   ON  (Dblote.NroPedido = Dbpedidoitem.Pedido)'
      '   AND  (Dblote.ItemPed = Dbpedidoitem.Item)'
      '   INNER JOIN "DBGRADE" dbgrade'
      '   ON (DbPedidoItem.CodGrade = dbGrade.Codigo)'
      '   INNER JOIN "DBPEDIDOMATERIAL.DB" Dbpedidomaterial'
      '   ON  (Dbpedidoitem.Pedido = Dbpedidomaterial.Pedido)'
      '   AND  (Dbpedidoitem.Item = Dbpedidomaterial.ItemPed)'
      '   INNER JOIN "dbMaterial.DB" Dbmaterial'
      '   ON  (Dbpedidomaterial.CodMaterial = Dbmaterial.Codigo)'
      'WHERE   Dblote.Lote BETWEEN :CodIni AND :CodFin'
      '   AND  (Dbpedidomaterial.CodMaterial = :Material)  '
      'ORDER BY Dblote.Lote Asc'
      ''
      ' '
      ' ')
    Left = 8
    Top = 448
