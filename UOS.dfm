  object qMateriais: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT Dbordemcompraitem.Codigo, Dbordemcompraitem.CodMaterial, ' +
        'Dbmaterial.Nome'
      'FROM "dbOrdemCompraItem.DB" Dbordemcompraitem'
      '   INNER JOIN "dbMaterial.DB" Dbmaterial'
      '   ON  (Dbordemcompraitem.CodMaterial = Dbmaterial.Codigo)  '
      
        'WHERE Dbordemcompraitem.QtdRestante > 0 AND Dbordemcompraitem.Co' +
        'digo = :NOC1'
      
