  object Query1: TQuery
    DatabaseName = 'Tabelas'
    SQL.Strings = (
      
        'SELECT Dbordemcompraitem.Codigo, Dbordemcompraitem.CodCor, Dbcor' +
        '.Nome'
      'FROM "dbOrdemCompraItem.DB" Dbordemcompraitem'
      '   INNER JOIN "Dbcor.DB" Dbcor'
      '   ON  (Dbordemcompraitem.CodCor = Dbcor.Codigo)  '
      
        'WHERE Dbordemcompraitem.QtdRestante > 0  AND Dbordemcompraitem.C' +
        'odigo = :C1 '
      '  AND  Dbordemcompraitem.CodMaterial = :C2'
      
        'GROUP BY Dbordemcompraitem.Codigo, Dbordemcompraitem.CodCor, Dbc' +
        'or.Nome'
      'ORDER BY Dbordemcompraitem.Codigo, Dbcor.Nome')
    Left = 190
    Top = 450
    ParamData = <
      item
        DataType = ftInteger
        Name = 'C1'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'C2'
        ParamType = ptUnknown
      end>
    object Query1Codigo: TIntegerField
      FieldName = 'Codigo'
      Origin = 'TABELAS."dbOrdemCompraItem.DB".Codigo'
    end
